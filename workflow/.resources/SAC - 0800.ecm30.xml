      <eventDescription>function beforeStateLeave(sequenceId){&#xd;	&#xd;	var numEmpresa = getValue(&quot;WKCompany&quot;);&#xd;	var numProcesso = getValue(&quot;WKNumProces&quot;);&#xd;	var nrProxAtividade = getValue(&quot;WKNextState&quot;);&#xd;	var numAtividade = sequenceId;&#xd;	var numThread = hAPI.getActualThread(numEmpresa, numProcesso, numAtividade);&#xd;	var colleagueId = getValue(&quot;WKUser&quot;);&#xd;	&#xd;	log.info(&quot;----------------------SET TASK COMENT-------------------------------&quot;);&#xd;	log.info(&quot;numEmpresa: &quot; + numEmpresa);&#xd;	log.info(&quot;numProcesso: &quot; + numProcesso);&#xd;	log.info(&quot;nrProxAtividade: &quot; + nrProxAtividade);&#xd;	log.info(&quot;numThread: &quot; + numThread);&#xd;	log.info(&quot;numAtividade: &quot; + numAtividade);&#xd;	log.info(&quot;colleagueId: &quot; + colleagueId);&#xd;	&#xd;	if(numAtividade == 0 || numAtividade == 1){ //abertura do atendimento&#xd;		log.info(&quot;numAtividade: &quot; + numAtividade);&#xd;		log.info(&quot;setTaskComments: &quot; + hAPI.getCardValue(&quot;desc_recla&quot;));&#xd;		hAPI.setTaskComments(colleagueId, numProcesso,  numThread,  hAPI.getCardValue(&quot;desc_recla&quot;));		&#xd;	}&#xd;	&#xd;	if(numAtividade == 11){ //propor ação corretiva&#xd;		hAPI.setTaskComments(colleagueId, numProcesso,  numThread,  hAPI.getCardValue(&quot;desc_acao&quot;));&#xd;		log.info(&quot;numAtividade: &quot; + numAtividade);&#xd;		log.info(&quot;setTaskComments: &quot; + hAPI.getCardValue(&quot;desc_acao&quot;));&#xd;	}&#xd;	&#xd;	if(numAtividade == 3){ //notificar cliente&#xd;		hAPI.setTaskComments(colleagueId, numProcesso,  numThread,  hAPI.getCardValue(&quot;obs_aceite&quot;));&#xd;		log.info(&quot;numAtividade: &quot; + numAtividade);&#xd;		log.info(&quot;setTaskComments: &quot; + hAPI.getCardValue(&quot;obs_aceite&quot;));&#xd;	}&#xd;	&#xd;	if(numAtividade == 5){ //implementar plano e ação&#xd;		setTaskComments(colleagueId, numProcesso,  numThread,  hAPI.getCardValue(&quot;obs_exec&quot;));&#xd;		log.info(&quot;numAtividade: &quot; + numAtividade);&#xd;		log.info(&quot;setTaskComments: &quot; + hAPI.getCardValue(&quot;obs_exec&quot;));&#xd;	}&#xd;	&#xd;	if(numAtividade == 6){ //implementar plano e ação&#xd;		hAPI.setTaskComments(colleagueId, numProcesso,  numThread,  hAPI.getCardValue(&quot;obs_encer&quot;));&#xd;		log.info(&quot;numAtividade: &quot; + numAtividade);&#xd;		log.info(&quot;setTaskComments: &quot; + hAPI.getCardValue(&quot;obs_encer&quot;));&#xd;	}&#xd;&#xd;}&#xd;</eventDescription>
